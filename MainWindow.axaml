<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="600"
        Background="AliceBlue"
        x:Class="AOP_Homework.MainWindow"
        x:Name="Main_Window">
        
        <StackPanel>
            <TextBlock x:Name="MessageBox" Foreground="Black"/>
            <Canvas x:Name="Canvas" PointerPressed="Canvas_PointerPressed"/>
            <Button Content="Hello" Click="Load" Background="DimGray"/>
            <Button Content="Hello2" Click="Button_Click" Background="DimGray"/>
        </StackPanel>
</Window>
